1031
#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);
    printf("%o", n);
    return 0;
}


1032
#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);
    printf("%x", n);
    return 0;
}


